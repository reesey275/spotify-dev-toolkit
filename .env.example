
# Spotify API credentials
# Get these from https://developer.spotify.com/dashboard
SPOTIFY_CLIENT_ID=your_client_id_here
SPOTIFY_CLIENT_SECRET=your_client_secret_here
SPOTIFY_REDIRECT_URI=http://127.0.0.1:5500/callback

# User identification (for demo playlists)
# SPOTIFY_USER_ID is optional and used for demo exports only
SPOTIFY_USER_ID=your_spotify_user_id

# App environment
NODE_ENV=development
PORT=5500
SESSION_SECRET=your_session_secret_here
DOCKER_CONTAINER=false
APP_UID=1000
APP_GID=1000

# Third-party integrations
LOGFIRE_API_KEY=your_logfire_api_key_here
CLOUDFLARED_TOKEN=your_cloudflare_tunnel_token_here